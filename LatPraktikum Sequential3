package Praktikum;

public class No3 {

    public static void main(String[] args) {
        int max = 0;
        int[] data = new int[]{48, 89, 12, 34, 52};
        System.out.println("Data di Array adalah : ");
        for (int i = 0; i < data.length; i++) {
            System.out.println(data[i] + "\t");
            if (data[i] > max) {
                max = data[i];
            }
        }
        System.out.println("\n Data terbesar adalah : " + max);
    }
}
